def reverse_words_order_and_swap_cases(sentence):
    words=sentence.split()
    words.reverse()
    return words
        
x=input()
func=reverse_words_order_and_swap_cases(x)
for i in func:
    print(i.swapcase(),end=" ") ##python program to print the given string in reverse order(only the words are reversed) changing their 
                                 ##respective cases(i.e., lowercase to uppercase and vice-versa)
                                 ##swapcase() is used to reverse the cases of alphabets
